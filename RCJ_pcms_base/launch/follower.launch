<launch>
    <arg name="image_source" default="/image_raw/compressed"/>
    <node
        name="YD"
        pkg="rcj_pcms_base"
        type="YOLODetectionNode.py"
        required="true"
        output="screen"
    ><param name="image_source" type="str" value="$(arg image_source)"/></node>
    <node
        name="pf_initializer"
        pkg="rcj_pcms_base"
        type="PFInitializer.py"
        required="false"
        output="screen"
    />
    <node
        name="person_follower"
        pkg="rcj_pcms_base"
        type="PersonFollower.py"
        required="true"
        output="screen"
    />
    <node
        name="waypoint_recorder"
        pkg="rcj_pcms_base"
        type="WayPointRecording.py"
        required="true"
        output="screen"
    />
</launch>
